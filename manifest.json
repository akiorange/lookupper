{
    "name": "Lookupper",
    "version": "1.0",
    "description": "Look up words quickly.",
    "permissions": ["<all_urls>", "tabs", "storage", "contextMenus"],
    "background": {
        "scripts": ["./chromeExtensionAsync-master/chrome-extension-async.js", "./background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/book-open-6c757d.png",
        "48": "icons/book-open-6c757d.png",
        "128": "icons/book-open-6c757d.png"
    },
    "manifest_version": 2
}
